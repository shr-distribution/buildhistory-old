#!/bin/sh\nif ! [ -e $D/etc/pango ] ; then\n\tmkdir -p $D/etc/pango\nfi\n\nif [ "x$D" != "x" ]; then\n\tPSEUDO_UNLOAD=1 qemu-x86_64 -L $D -E LD_LIBRARY_PATH=$D/usr/lib:$D/lib $D/usr/bin/pango-querymodules \\\n\t\t$D/usr/lib/pango/1.8.0/modules/*.so \\\n\t\t> $D/etc/pango/pango.modules 2>/dev/null\n\n\t[ $? -ne 0 ] && exit 1\n\n\tsed -i -e "s:$D::" $D/etc/pango/pango.modules\n\n\texit 0\nfi\n/usr/bin/pango-querymodules > /etc/pango/pango.modules\n