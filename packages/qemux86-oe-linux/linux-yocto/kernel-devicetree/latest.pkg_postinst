\tcd /boot\n\tfor DTB_FILE in ${KERNEL_DEVICETREE}\n\tdo\n\t\tDTB_BASE_NAME=`basename ${DTB_FILE} | awk -F "." \'{print $1}\'`\n\t\tDTB_SYMLINK_NAME=`echo bzImage-qemux86 | sed "s/qemux86/${DTB_BASE_NAME}/g"`\n\t\tupdate-alternatives --install /boot/${DTB_BASE_NAME}.dtb ${DTB_BASE_NAME}.dtb devicetree-${DTB_SYMLINK_NAME}.dtb 31025 || true\n\tdone\n