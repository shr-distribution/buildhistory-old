    deluser gdm || true
    delgroup gdm || true
	sed -i /gdm/d /etc/X11/default-display-manager || true
if [ "$D" != "" ]; then
	update-rc.d -f -r $D gdm remove
else
	update-rc.d gdm remove
fi
for icondir in /usr/share/icons/* ; do
    if [ -d $icondir ] ; then
        gtk-update-icon-cache -qt  $icondir
    fi
done
