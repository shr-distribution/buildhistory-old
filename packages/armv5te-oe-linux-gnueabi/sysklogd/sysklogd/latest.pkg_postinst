#!/bin/sh
	update-alternatives --install /sbin/syslogd syslogd /sbin/syslogd.sysklogd 100
	update-alternatives --install /sbin/klogd klogd /sbin/klogd.sysklogd 100
	update-alternatives --install /etc/init.d/syslog syslog-init /etc/init.d/syslog.sysklogd 100
	update-alternatives --install /etc/syslog.conf syslog-conf /etc/syslog.conf.sysklogd 100
