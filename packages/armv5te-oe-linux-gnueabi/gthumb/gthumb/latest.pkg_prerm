#!/bin/sh
SCHEMA_LOCATION=/etc/gconf/schemas
for SCHEMA in gthumb_webalbums.schemas gthumb-image-viewer.schemas gthumb_resize_options.schemas gthumb_rename_series.schemas gthumb-importer.schemas gthumb_image_print.schemas gthumb_convert_format.schemas gthumb-comments.schemas gthumb_crop_options.schemas gthumb.schemas gthumb_photo_importer.schemas gthumb_resize_images.schemas gthumb-slideshow.schemas gthumb-pixbuf-savers.schemas gthumb-gstreamer.schemas; do
	if [ -e $SCHEMA_LOCATION/$SCHEMA ]; then
		HOME=/root GCONF_CONFIG_SOURCE=`gconftool-2 --get-default-source` \
			gconftool-2 \
			--makefile-uninstall-rule $SCHEMA_LOCATION/$SCHEMA > /dev/null
	fi
done
