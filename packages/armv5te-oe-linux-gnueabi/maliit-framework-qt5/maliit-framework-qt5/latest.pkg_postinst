#!/bin/sh\n# should run online\nif [ "x$D" != "x" ]; then\n    exit 1\nfi\necho "export QT_IM_MODULE=Maliit" >> /etc/xprofile\nln -s /usr/share/applications/maliit-server.desktop /etc/xdg/autostart/maliit-server.desktop\nif [ x"$D" = "x" ]; then\n\tif [ -x /sbin/ldconfig ]; then /sbin/ldconfig ; fi\nfi\n