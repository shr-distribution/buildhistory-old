#!/bin/sh
	update-alternatives --install /usr/bin/addr2line addr2line /usr/bin/arm-oe-linux-gnueabi-addr2line 100
	update-alternatives --install /usr/bin/ar ar /usr/bin/arm-oe-linux-gnueabi-ar 100
	update-alternatives --install /usr/bin/as as /usr/bin/arm-oe-linux-gnueabi-as 100
	update-alternatives --install /usr/bin/c++filt c++filt /usr/bin/arm-oe-linux-gnueabi-c++filt 100
	update-alternatives --install /usr/bin/elfedit elfedit /usr/bin/arm-oe-linux-gnueabi-elfedit 100
	update-alternatives --install /usr/bin/gprof gprof /usr/bin/arm-oe-linux-gnueabi-gprof 100
	update-alternatives --install /usr/bin/ld ld /usr/bin/arm-oe-linux-gnueabi-ld 100
	update-alternatives --install /usr/bin/ld.gold ld.gold /usr/bin/arm-oe-linux-gnueabi-ld.gold 100
	update-alternatives --install /usr/bin/nm nm /usr/bin/arm-oe-linux-gnueabi-nm 100
	update-alternatives --install /usr/bin/objcopy objcopy /usr/bin/arm-oe-linux-gnueabi-objcopy 100
	update-alternatives --install /usr/bin/objdump objdump /usr/bin/arm-oe-linux-gnueabi-objdump 100
	update-alternatives --install /usr/bin/ranlib ranlib /usr/bin/arm-oe-linux-gnueabi-ranlib 100
	update-alternatives --install /usr/bin/readelf readelf /usr/bin/arm-oe-linux-gnueabi-readelf 100
	update-alternatives --install /usr/bin/size size /usr/bin/arm-oe-linux-gnueabi-size 100
	update-alternatives --install /usr/bin/strings strings /usr/bin/arm-oe-linux-gnueabi-strings 100
	update-alternatives --install /usr/bin/strip strip /usr/bin/arm-oe-linux-gnueabi-strip 100
if [ x"$D" = "x" ]; then
	if [ -x /sbin/ldconfig ]; then /sbin/ldconfig ; fi
fi
