#!/bin/sh\n    if [ -z "$D" ]; then\n        if type update-rc.d >/dev/null 2>/dev/null; then\n            if [ -f /etc/init.d/dbus-1 ]; then\n                /etc/init.d/dbus-1 reload || true\n            fi\n        fi\n    fi\nif type update-rc.d >/dev/null 2>/dev/null; then\n\tif [ -n "$D" ]; then\n\t\tOPT="-r $D"\n\telse\n\t\tOPT="-s"\n\tfi\n\tupdate-rc.d $OPT atd defaults 97\nfi\n