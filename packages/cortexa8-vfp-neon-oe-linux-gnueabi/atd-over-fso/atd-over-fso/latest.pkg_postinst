#!/bin/sh
    if [ -z "$D" ]; then
        if type update-rc.d >/dev/null 2>/dev/null; then
            if [ -f /etc/init.d/dbus-1 ]; then
                /etc/init.d/dbus-1 reload || true
            fi
        fi
    fi
if test "x$D" != "x"; then
	OPT="-r $D"
else
	OPT="-s"
fi
if type update-rc.d >/dev/null 2>/dev/null; then
	update-rc.d $OPT atd defaults 97
fi
